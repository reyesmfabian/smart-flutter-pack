{
  "name": "smart-flutter-pack",
  "displayName": "Smart Flutter Pack",
  "description": "This extension pack allows you to accelerate the development of your Flutter projects.",
  "version": "1.0.0",
  "icon": "icon/icon.png",
  "repository": "https://github.com/reyesmfabian/smart-flutter-pack",
  "publisher": "FabianReyes",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Extension Packs"
  ],
  "extensionPack": [
    "dart-code.flutter",
    "streetsidesoftware.code-spell-checker",
    "streetsidesoftware.code-spell-checker-spanish",
    "vsls-contrib.codetour",
    "dart-code.dart-code",
    "miquelddg.dart-barrel-file-generator",
    "cweijan.vscode-office",
    "esbenp.prettier-vscode",
    "fabianreyes.smart-json-schema"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
